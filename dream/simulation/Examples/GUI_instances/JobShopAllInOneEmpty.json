        },
        "confidenceLevel" : {
          "title" : "Confidence level",
          "type" : "number",
          "default" : 0.95,
          "description" : "Confidence level for statistical analysis of stochastic experiments"
        },
        "processTimeout" : {
          "title" : "Process Timeout",
