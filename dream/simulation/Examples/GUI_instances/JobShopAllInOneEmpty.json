      "_schedulingRule" : {
        "name": "Scheduling Rule",
        "description" : "Scheduling Rule, one of FIFO Priority EDD EOD NumStages RPC LPT SPT MS WINQ",
        "type" : "string",
        "default" : "FIFO",
        "enum" : [
          "FIFO",
          "Priority",
          "EDD",
          "EOD",
          "NumStages",
          "RPC",
          "LPT",
          "SPT",
          "MS",
          "WINQ",
          "WT"
        ]
      },
      "_capacity" : {
        "name": "Capacity",
        "description" : "capacity of the queue. -1 means infinite",
        "type" : "number",
        "default" : 1,
        "oneOf" : [{
            "enum" : [
              -1
            ]
          }, {
            "multipleOf" : 1
          }
        ]
      },
      "_operationType" : {
        "name" : "Operation type",
