    "Dream.RoutingQueue": {
      "_class": "node", 
      "allOf": [
        {
          "$ref": "#/node"
        }, 
        {
          "properties": {
            "capacity": {
              "$ref": "#/definitions/_capacity", 
              "required": true
            }, 
